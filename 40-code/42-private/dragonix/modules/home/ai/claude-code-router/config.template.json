{
  "LOG_LEVEL": "debug",
  "HOST": "127.0.0.1",
  "PORT": 3456,
  "APIKEY": "",
  "API_TIMEOUT_MS": "600000",
  "PROXY_URL": "",
  "transformers": [
    {
      "path": "~/.claude-code-router/plugins/musistudio-gemini-cli.js",
      "options": {
        "project": "gen-lang-client-0577327587"
      }
    },
    {
      "path": "~/.claude-code-router/plugins/dabstractor-z-ai.js"
    },
    {
      "path": "~/.claude-code-router/plugins/dabstractor-z-ai-websearch.js",
      "options": {
        "endpoint": "https://api.z.ai/api/mcp/web_search_prime/mcp",
        "timeout": 60000
      }
    }
  ],
  "Providers": [
    {
      "name": "openrouter",
      "api_base_url": "https://openrouter.ai/api/v1/chat/completions",
      "api_key": "$OPENROUTER_API_KEY",
      "models": [
        "x-ai/grok-code-fast-1",
        "x-ai/grok-4.1-fast:free",
        "google/gemini-2.5-pro-preview"
      ],
      "transformer": {
        "use": [
          "openrouter"
        ]
      }
    },
    {
      "name": "zai",
      "api_base_url": "https://api.z.ai/api/anthropic/v1/messages",
      "api_key": "$ZAI_API_KEY",
      "models": [
        "glm-4.6",
        "glm-4.5",
        "glm-4.5-air"
      ],
      "transformer": {
        "use": [
          "Anthropic"
        ]
      }
    },
    {
      "name": "gemini-cli",
      "api_base_url": "https://cloudcode-pa.googleapis.com/v1internal",
      "api_key": "*",
      "models": [
        "gemini-2.5-flash"
      ],
      "transformer": {
        "use": [
          "gemini-cli"
        ]
      }
    },
    {
      "name": "z-ai-mcp-websearch",
      "api_base_url": "http://localhost:37891",
      "api_key": "*",
      "transformer": {
        "use": [
          "z-ai-websearch"
        ]
      },
      "models": [
        "websearch"
      ]
    }
  ],
  "StatusLine": {
    "enabled": false,
    "currentStyle": "default",
    "default": {
      "modules": []
    },
    "powerline": {
      "modules": []
    }
  },
  "Router": {
    "default": "zai,glm-4.6",
    "background": "zai,glm-4.5-air",
    "think": "zai,glm-4.6",
    "longContext": "zai,glm-4.6",
    "longContextThreshold": 60000,
    "webSearch": "z-ai-mcp-websearch,websearch",
    "image": "zai,glm-4.6"
  },
  "CUSTOM_ROUTER_PATH": ""
}
