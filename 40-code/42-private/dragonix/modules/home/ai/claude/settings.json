{
  "permissions": {
    "allow": [
      "WebFetch",
      "WebSearch",
      "Read(*)",
      "Skill(*)"
    ],
    "deny": [
      "SlashCommand"
    ]
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/langfuse_hook.py"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh",
    "padding": 0
  },
  "enabledPlugins": {
    "plugin-dev@claude-code-plugins": true,
    "agent-sdk-dev@claude-code-plugins": false,
    "document-skills@anthropic-agent-skills": false,
    "example-skills@anthropic-agent-skills": false,
    "web-research@gtd-cc": true,
    "developing-for-claude-code@gtd-cc": false,
    "documentation@gtd-cc": false,
    "github@gtd-cc": false,
    "obsidian@gtd-cc": false,
    "random@gtd-cc": true,
    "doc-parsing@gtd-cc": false,
    "ralph-wiggum@claude-code-plugins": false
  },
  "outputStyle": "Explanatory",
  "alwaysThinkingEnabled": true
}
